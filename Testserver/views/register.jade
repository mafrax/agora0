    .header-spacer--standard
    .container
      .row.display-flex
        .col.col-xl-6.col-lg-6.col-md-12.col-sm-12.col-12
          .landing-content
            h1 Welcome to the Biggest Social Network in the World
            | 				
            p
              | We are the best and biggest social network with 5 billion active users all around the world. Share you
              | thoughts, write blog posts, show your favourite music via Stopify, earn badges and much more!
            | 				
            a.btn.btn-md.btn-border.c-white(href="#") Register Now!
        | 		
        .col.col-xl-5.col-lg-6.col-md-12.col-sm-12.col-12
          // Login-Registration Form
          .registration-login-form
            // Nav tabs
            ul.nav.nav-tabs(role="tablist")
              li.nav-item
                a.nav-link.active(data-toggle="tab", href="olympus/src/#home", role="tab")
                  svg.olymp-login-icon
                    use(xlink:href="olympus/src/svg-icons/sprites/icons.svg#olymp-login-icon")
              | 					
              li.nav-item
                a.nav-link(data-toggle="tab", href="olympus/src/#profile", role="tab")
                  svg.olymp-register-icon
                    use(xlink:href="olympus/src/svg-icons/sprites/icons.svg#olymp-register-icon")
            // Tab panes
            .tab-content
              #home.tab-pane.active(role="tabpanel", data-mh="log-tab")
                .title.h6 Register to Olympus
                | 						
                form.content
                  .row
                    .col.col-12.col-xl-6.col-lg-6.col-md-6.col-sm-12
                      .form-group.label-floating.is-empty
                        label.control-label First Name
                        | 										
                        input.form-control(placeholder="", type="text")
                    | 								
                    .col.col-12.col-xl-6.col-lg-6.col-md-6.col-sm-12
                      .form-group.label-floating.is-empty
                        label.control-label Last Name
                        | 										
                        input.form-control(placeholder="", type="text")
                    | 								
                    .col.col-12.col-xl-12.col-lg-12.col-md-12.col-sm-12
                      .form-group.label-floating.is-empty
                        label.control-label Your Email
                        | 										
                        input.form-control(placeholder="", type="email")
                      | 									
                      .form-group.label-floating.is-empty
                        label.control-label Your Password
                        | 										
                        input.form-control(placeholder="", type="password")
                      | 			
                      | 									
                      .form-group.date-time-picker.label-floating
                        label.control-label Your Birthday
                        | 										
                        input(name="datetimepicker", value="10/24/1984")
                        | 										
                        span.input-group-addon
                          svg.olymp-calendar-icon
                            use(xlink:href="olympus/src/svg-icons/sprites/icons.svg#olymp-calendar-icon")
                      | 			
                      | 									
                      .form-group.label-floating.is-select
                        label.control-label Your Gender
                        | 										
                        select.selectpicker.form-control
                          option(value="MA") Male
                          | 											
                          option(value="FE") Female
                      | 			
                      | 									
                      .remember
                        .checkbox
                          label
                            input(name="optionsCheckboxes", type="checkbox")
                            | 												I accept the 
                            a(href="#") Terms and Conditions
                            |  of the website
                      | 			
                      | 									
                      a.btn.btn-purple.btn-lg.full-width(href="#") Complete Registration!
              | 			
              | 					
              #profile.tab-pane(role="tabpanel", data-mh="log-tab")
                .title.h6 Login to your Account
